set background=dark
" color solarized
color molokai
let g:molokai_original = 1

set autoindent
set number
set hlsearch
set ignorecase

set mouse=a

set hidden
set history=100
set wildmode=list:longest
set ignorecase
set smartcase
set title
set backupdir=~/.vim-tmp,~/.tmp,~/tmp,/var/tmp,/tmp
set directory=~/.vim-tmp,~/.tmp,~/tmp,/var/tmp,/tmp
syntax on
filetype on
filetype plugin on
filetype indent on
set hlsearch
set incsearch
" set list
" set listchars=tab:>-,trail:>.,eol:$

set expandtab
set tabstop=2
set shiftwidth=2
set guifont=Monospace:20

" set columns=175 "full width
" set lines=53 "full height
" set equalalways
" vnew
